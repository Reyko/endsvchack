  <script type="text/javascript"
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBa6pCZqs-padkcoNkzOOGtkMdjDp8NtRc">
  </script>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <$= stylesheet_link_tag 'main'%>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body cz-shortcut-listen="true">
  <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">

      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      <%= link_to "#endsvchack",'/',:class =>"navbar-brand"%>
      </div>
      <div class="navbar-collapse collapse">
        <form class="navbar-form navbar-right" role="form">
          <ul class="nav navbar-nav">
        <li class="active"><%= link_to "Register",new_user_registration_path %></li>
        <li><%=link_to 'Sign in',user_session_path %></li>
      </ul>
          <div class="form-group">
            <input type="text" placeholder="Email" class="form-control">
          </div>
          <div class="form-group">
            <input type="password" placeholder="Password" class="form-control">
          </div>
          <button type="submit" class="btn btn-success">Sign in</button>
        </form>
      </div><!--/.navbar-collapse -->
 
  </div>

<% if notice %>
<p class="alert alert-warning fade in"><%= notice %></p>
<% end %>
<% if alert  %>
<p class="alert alert-success"><%= alert %></p>
<% end %>
<%= yield %>


<footer>
